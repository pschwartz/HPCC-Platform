<?xml version='1.0'?>
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog" prefer="system">

<rewriteSystem
        systemIdStartString="http://www.oasis-open.org/docbook/xml/4.5/"
        rewritePrefix="${DOCBOOK_RESOURCES}/docbook-xml/" />
<rewriteURI
        uriStartString="http://docbook.sourceforge.net/release/xsl/current/"
        rewritePrefix="${DOCBOOK_RESOURCES}/docbook-xsl/" />

<rewriteSystem 
	systemIdStartString="images/" 
	rewritePrefix="${DOC_IMAGES}"/>

<rewriteURI 
	uriStartString="images/" 
	rewritePrefix="${DOC_IMAGES}"/>

<uri name="images/" uri="${DOC_IMAGES}" />
   

</catalog>
